      "steps": [
        {
          "order": 1,
          "title": "Chop the Onion",
          "description": "Dice 1/4 of an onion and set aside."
        },
        {
          "order": 2,
          "title": "Add Ingredients",
          "description": "In a bowl, add 4 crab sticks, 3T diced onion, 4T mayonnaise, 2T honey mustard, and 1T sweet relish."
        },
        {
          "order": 3,
          "title": "Season with Salt and Pepper",
          "description": "Add a pinch of salt and pepper."
        },
        {
          "order": 4,
          "title": "Mix Enthusiastically!",
          "description": "Put on plastic gloves. Now mash and mix the crab sticks thoroughly!"
        },
        {
          "order": 5,
          "title": "Fill the Bread",
          "description": "Fill the bread rolls with the prepared crab salad."
        },
        {
          "order": 6,
          "title": "Done!",
          "description": "Enjoy your delicious crab stick sandwich!"
        }
      ]
